<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddUserController">
   <children>
      <Label layoutX="243.0" layoutY="14.0" text="Add a user">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="btnBack" layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#goToLastWindow" text="Go back" />
      <Button fx:id="btnAdd" layoutX="243.0" layoutY="336.0" mnemonicParsing="false" onAction="#addUser" prefHeight="25.0" prefWidth="101.0" text="Add" />
      <TextField fx:id="textName" layoutX="219.0" layoutY="91.0" promptText="Name" />
      <TextField fx:id="textSurname" layoutX="219.0" layoutY="125.0" promptText="Surname" />
      <TextField fx:id="textLogin" layoutX="219.0" layoutY="160.0" promptText="Login name" />
      <TextField fx:id="textPsw" layoutX="219.0" layoutY="194.0" promptText="Password" />
      <TextField fx:id="textEmail" layoutX="219.0" layoutY="231.0" promptText="Email" />
      <TextField fx:id="textNum" layoutX="219.0" layoutY="264.0" promptText="Phone number" />
      <ComboBox fx:id="comboType" layoutX="219.0" layoutY="296.0" prefWidth="150.0" promptText="User type" />
   </children>
</AnchorPane>
