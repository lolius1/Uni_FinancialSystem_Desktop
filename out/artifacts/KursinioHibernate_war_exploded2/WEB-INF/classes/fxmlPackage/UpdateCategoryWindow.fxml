<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="264.0" prefWidth="396.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UpdateCategoryController">
   <children>
      <Label layoutX="120.0" layoutY="14.0" text="Category update">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="textName" layoutX="124.0" layoutY="75.0" promptText="Category name" />
      <TextField fx:id="textDescription" layoutX="124.0" layoutY="120.0" promptText="Category description" />
      <Button fx:id="btnUpdate" layoutX="124.0" layoutY="188.0" mnemonicParsing="false" onAction="#updateCategory" prefHeight="25.0" prefWidth="149.0" text="Update" />
      <Button fx:id="btnBack" layoutX="14.0" layoutY="225.0" mnemonicParsing="false" onAction="#goToLastWindow" text="Go back" />
   </children>
</AnchorPane>
