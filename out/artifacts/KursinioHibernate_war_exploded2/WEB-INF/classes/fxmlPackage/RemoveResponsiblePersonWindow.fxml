<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="248.0" prefWidth="299.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RemoveResponsiblePersonController">
   <children>
      <Label layoutX="20.0" layoutY="14.0" text="Remove responsible person">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="btnBack" layoutX="14.0" layoutY="209.0" mnemonicParsing="false" onAction="#goToLastWindow" text="Go back" />
      <ComboBox fx:id="comboID" layoutX="75.0" layoutY="78.0" prefWidth="150.0" promptText="User ID" />
      <Button fx:id="btnRemove" layoutX="120.0" layoutY="153.0" mnemonicParsing="false" onAction="#removeResponsiblePerson" text="Remove" />
   </children>
</AnchorPane>
