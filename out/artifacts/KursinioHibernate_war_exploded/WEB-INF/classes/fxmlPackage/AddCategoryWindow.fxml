<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="367.0" prefWidth="519.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddCategoryController">
   <children>
      <Label layoutX="195.0" layoutY="14.0" text="New category">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="textName" layoutX="186.0" layoutY="114.0" promptText="Category name" />
      <TextField fx:id="textDescription" layoutX="186.0" layoutY="159.0" promptText="Category description" />
      <Button fx:id="btnBack" layoutX="13.0" layoutY="328.0" mnemonicParsing="false" onAction="#goToLastWindow" text="Go back" />
      <Button fx:id="btnAdd" layoutX="186.0" layoutY="253.0" mnemonicParsing="false" onAction="#addCategory" prefHeight="25.0" prefWidth="149.0" text="Add" />
      <ComboBox fx:id="comboParent" layoutX="186.0" layoutY="201.0" prefWidth="150.0" promptText="Parent category" />
   </children>
</AnchorPane>
