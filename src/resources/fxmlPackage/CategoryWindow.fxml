<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="452.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CategoryController">
   <children>
      <Label layoutX="119.0" layoutY="14.0" text="Category management">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="btnAdd" layoutX="8.0" layoutY="301.0" mnemonicParsing="false" onAction="#addCategoryWindow" prefHeight="25.0" prefWidth="437.0" text="Add a category" />
      <Button fx:id="btnAddTo" layoutX="295.0" layoutY="188.0" mnemonicParsing="false" onAction="#addToCategoryWindow" prefHeight="25.0" prefWidth="147.0" text="Add to a category" />
      <Button fx:id="btnUpd" layoutX="295.0" layoutY="117.0" mnemonicParsing="false" onAction="#updateCategoryWindow" prefHeight="25.0" prefWidth="147.0" text="Update a category" />
      <Button fx:id="btnRem" layoutX="295.0" layoutY="151.0" mnemonicParsing="false" onAction="#removeFromCategoryWindow" text="Remove from a category" />
      <Button fx:id="btnBack" layoutX="8.0" layoutY="361.0" mnemonicParsing="false" onAction="#goToLastWindow" text="Go back" />
      <Button fx:id="btnShow" layoutX="8.0" layoutY="234.0" mnemonicParsing="false" onAction="#showCategoriesWindow" prefHeight="25.0" prefWidth="437.0" text="Show categories" />
      <Button fx:id="btnDel" layoutX="295.0" layoutY="81.0" mnemonicParsing="false" onAction="#deleteCategory" prefHeight="25.0" prefWidth="147.0" text="Delete a category" />
      <ComboBox fx:id="comboCat" layoutX="14.0" layoutY="81.0" prefHeight="25.0" prefWidth="187.0" promptText="Category name" />
      <Button fx:id="btnFilter" layoutX="8.0" layoutY="268.0" mnemonicParsing="false" onAction="#loadFilterCategories" prefHeight="25.0" prefWidth="437.0" text="Filter categories" />
   </children>
</AnchorPane>
